# Environment Variables for OTP Bot System
# Copy this file to .env and fill in your values

# Telegram Bot Token (required)
BOT_TOKEN=your_bot_token_here

# Port for health server (optional, defaults to 10000)
PORT=10000

# Admin User ID (required for admin approval system)
# This is your Telegram user ID - get it by messaging @userinfobot
ADMIN_USER_ID=7325836764
